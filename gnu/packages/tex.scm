        #~(let ((delete-other-directories
                 (lambda (root dirs)
                   (with-directory-excursion root
                     (for-each
                      delete-file-recursively
                      (scandir "."
                               (lambda (file)
                                 (and (not (member file (append '("." "..") dirs)))
                                      (eq? 'directory (stat:type (stat file)))))))))))
            (delete-other-directories "libs" '())
