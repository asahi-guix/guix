    (outputs '("out" "doc"))
    (build-system texlive-build-system)
    (arguments
     (list
      #:modules '((ice-9 match)
                  (ice-9 regex)
                  (srfi srfi-1)
                  (guix build texlive-build-system)
                  (guix build utils))
      #:build-targets #~(list "revtex4-1.dtx")
      #:phases
      #~(modify-phases %standard-phases
          ;; In order to build revtex4-1, we need files from revtex source.
